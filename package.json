{
    "private": true,
    "workspaces": [
        "packages/*",
        "website"
    ],
    "devDependencies": {
        "nx": "15.6.3"
    },
    "scripts": {
        "clear": "nx run-many --target=clear && nx reset",
        "build": "nx run-many --target=build",
        "watch": "nx run-many --target=watch",
        "test": "nx run-many --target=test",
        "start": "cd website && yarn start"
    },
    "release": {
        "branches": [
            "main"
        ]
    },
    "version": "0.0.0",
    "volta": {
        "node": "18.15.0",
        "yarn": "1.22.19"
    }
}
