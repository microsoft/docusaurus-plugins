    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "docusaurus start",
            "skipFiles": ["<node_internals>/**"],
            "cwd": "${workspaceFolder}/website",
            "program": "node_modules/.bin/docusaurus",
            "args": ["start"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "docusaurus build",
            "skipFiles": ["<node_internals>/**"],
            "cwd": "${workspaceFolder}/website",
            "program": "node_modules/.bin/docusaurus",
            "args": ["build"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "code-tabs",
            "skipFiles": ["<node_internals>/**"],
            "cwd": "${workspaceFolder}/packages/docusaurus-remark-plugin-code-tabs",
            "program": "node_modules/.bin/jest",
            "args": [""]
        }
    ]
}
